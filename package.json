{
  "name": "setup-hashicorp-releases",
  "version": "2.0.0",
  "private": true,
  "description": "Setup a binary from releases.hashicorp.com in a GitHub Actions build environment",
  "main": "dist/setup/index.js",
  "scripts": {
    "package": "ncc build -o dist/setup src/setup.ts --source-map --license LICENSE && ncc build -o dist/cache-save src/cache-save.ts --source-map --license LICENSE",
    "test": "jest --coverage src/**/*",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloominlabs/setup-hashicorp-releases.git"
  },
  "author": "Bloomin' Labs",
  "license": "MPL-2.0",
  "dependencies": {
    "@actions/cache": "^3.1.3",
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "got": "^11",
    "jest-playback": "^3.0.0",
    "make-dir": "^3.1.0",
    "mkdirp-promise": "^5.0.1",
    "node-fetch": "^3.3.0",
    "runtypes": "^6.6.0",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.34.0",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
