{
  "name": "setup-hashicorp-releases",
  "version": "0.0.0",
  "private": true,
  "description": "Setup a binary from releases.hashicorp.com in a GitHub Actions build environment",
  "main": "dist/setup/index.js",
  "scripts": {
    "package": "ncc build -o dist/setup src/setup.ts --source-map --license LICENSE && ncc build -o dist/cache-save src/cache-save.ts --source-map --license LICENSE",
    "test": "jest --coverage src/**/*",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloominlabs/setup-hashicorp-releases.git"
  },
  "author": "Bloomin' Labs",
  "license": "MPL-2.0",
  "dependencies": {
    "@actions/cache": "^2.0.2",
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@actions/io": "^1.1.1",
    "@actions/tool-cache": "^1.7.1",
    "got": "^11.8.2",
    "jest-playback": "^3.0.0",
    "make-dir": "^3.1.0",
    "mkdirp-promise": "^5.0.1",
    "node-fetch": "^2.6.1",
    "runtypes": "^6.5.1",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.13.0",
    "jest": "^27.5.1",
    "ncc": "^0.3.6",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.3"
  }
}
